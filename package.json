{
  "name": "vscode-jsonl-viewer",
  "displayName": "JSONL Viewer",
  "description": "View and query JSONL/NDJSON files as interactive tables with SQL-like filtering",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "mametora",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mametora/vscode-jsonl-viewer"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "jsonl",
    "ndjson",
    "json lines",
    "table",
    "viewer"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "jsonlViewer.editor",
        "displayName": "JSONL Viewer",
        "selector": [
          {
            "filenamePattern": "*.jsonl"
          },
          {
            "filenamePattern": "*.ndjson"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run build:extension && npm run build:webview",
    "build:extension": "node esbuild.js",
    "build:webview": "cd webview-ui && npm run build",
    "watch": "node esbuild.js --watch",
    "dev:webview": "cd webview-ui && npm run dev",
    "lint": "eslint src --ext ts",
    "pretest": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^4.0.18",
    "esbuild": "^0.25.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "node-sql-parser": "^5.3.6"
  }
}
